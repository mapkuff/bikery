{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": [
    "plugin:react-app/recommended"
  ],
  "plugins": [],
  "rules": {
    "react-app/jsx-a11y/href-no-hash": "off"
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
